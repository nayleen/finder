{
  "name": "nayleen/finder",
  "description": "Yet another class finder implementation.",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/finder": "^6|^7"
  },
  "require-dev": {
    "nayleen/codestandard": "dev-latest",
    "nayleen/development-dependencies": "^3.4.2",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Nayleen\\Finder\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nayleen\\Finder\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.0.x-dev"
    }
  }
}
