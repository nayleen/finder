{
    "name": "nayleen/finder",
    "description": "Yet another class finder implementation.",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nayleen/attribute": "^3"
    },
    "require-dev": {
        "nayleen/development": "^2@dev",
        "roave/better-reflection": "^6",
        "roave/security-advisories": "dev-latest"
    },
    "suggest": {
        "roave/better-reflection": "To use the BetterReflection engine"
    },
    "autoload": {
        "psr-4": {
            "Nayleen\\Finder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayleen\\Finder\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
